<div id="sidebar" class="box has-background-white">
    <div id="sidebar-brand" class="menu-label">
        <h1 class="title is-4">EvaTion</h1>
    </div>
    
    <ul class="menu-list">
        <% for(i = 0; i < list_of_page_attribute.length; i++){ %>
            <li>
                <a href="<%= list_of_page_attribute[i][0] %>">
                    <span class="icon-text">
                        <span class="icon">
                            <i class="fas <%= list_of_page_attribute[i][1] %>"></i>
                        </span>
                        
                        <span><%= list_of_page_attribute[i][0].charAt(0).toUpperCase() + list_of_page_attribute[i][0].slice(1) %></span>
                    </span>
                </a>
            </li>
        <% } %>
    </ul>
    
    <div id="copyright-container" class="pt-2 pb-2">
        <p class="is-size-6">Copyright Â© 2022 XI-RPL</p>
    </div>
</div>